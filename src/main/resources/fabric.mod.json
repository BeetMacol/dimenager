{
  "schemaVersion": 1,

  "id": "dimenager",
  "version": "0.0.1",
  "name": "Dimenager",
  "description": "A fabric mod that lets you manage dimensions in Minecraft. Modern alternative of Multiverse.",
  "authors": [
    "BeetMacol"
  ],
  "contact": {
    "sources": "https://github.com/BeetMacol/dimenager"
  },
  "license": "BSD-3-Clause-Clear",

  "environment": "server",
  "entrypoints": {
    "server": [
      "com.beetmacol.mc.dimenager.Dimenager::init"
    ]
  },
  "mixins": [
    "dimenager.mixins.json"
  ],
  "accessWidener": "dimenager.accesswidener",

  "depends": {
    "minecraft": "1.16.x",
    "fabricloader": ">=0.11.0"
  }
}